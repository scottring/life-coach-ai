{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "summary": "Dog behavior monitoring through Alexa sound detection",
          "examplePhrases": [
            "Alexa, tell Family Dashboard the dog is barking",
            "Alexa, ask Family Dashboard to start monitoring",
            "Alexa, tell Family Dashboard I'm leaving"
          ],
          "keywords": [
            "dog",
            "pet",
            "behavior",
            "monitoring",
            "bark",
            "family"
          ],
          "name": "Family Dashboard Dog Monitor",
          "description": "Monitor your dog's behavior while you're away using Alexa's sound detection capabilities. Track barking, movement, and anxiety levels to improve your pet's wellbeing.",
          "largeIconUri": "https://your-domain.com/icons/large-icon.png",
          "smallIconUri": "https://your-domain.com/icons/small-icon.png"
        }
      },
      "isAvailableWorldwide": false,
      "testingInstructions": "This skill monitors dog behavior through sound detection. Test by saying 'Alexa, tell Family Dashboard to start monitoring' then make barking sounds.",
      "category": "SMART_HOME",
      "distributionCountries": ["US"]
    },
    "apis": {
      "custom": {
        "endpoint": {
          "uri": "https://your-domain.com/api/alexa-webhook"
        },
        "interfaces": [
          {
            "type": "AUDIO_PLAYER"
          }
        ],
        "regions": {
          "NA": {
            "endpoint": {
              "uri": "https://your-domain.com/api/alexa-webhook"
            }
          }
        }
      },
      "smartHome": {
        "endpoint": {
          "uri": "https://your-domain.com/api/alexa-webhook"
        },
        "protocolVersion": "3"
      }
    },
    "manifestVersion": "1.0",
    "permissions": [
      {
        "name": "alexa::devices:all:notifications:write"
      },
      {
        "name": "alexa::profile:email:read"
      }
    ],
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "locales": {
        "en-US": {
          "termsOfUseUrl": "https://your-domain.com/terms",
          "privacyPolicyUrl": "https://your-domain.com/privacy"
        }
      },
      "containsAds": false,
      "isExportCompliant": true,
      "isChildDirected": false,
      "usesPersonalInfo": false
    }
  }
}